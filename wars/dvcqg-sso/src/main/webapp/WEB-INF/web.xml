<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

	<servlet>
		<description>
        </description>
		<display-name>callback</display-name>
		<servlet-name>DVCQGSSOCallbackServlet</servlet-name>
		<servlet-class>org.fds.opencps.dvcqg.authentication.DVCQGSSOCallbackServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DVCQGSSOCallbackServlet</servlet-name>
		<url-pattern>/callback</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MICSSOCallbackServlet</servlet-name>
		<servlet-class>org.fds.opencps.dvcqg.authentication.MICSSOCallbackServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SSOCSCallbackServlet</servlet-name>
		<servlet-class>org.fds.opencps.dvcqg.authentication.SSOCSCallbackServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MICSSOCallbackServlet</servlet-name>
		<url-pattern>/callbackMic</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SSOCSCallbackServlet</servlet-name>
		<url-pattern>/ssocscallback</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>/DocumentPorcessorServlet</welcome-file>
	</welcome-file-list>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>
				JSP MVC Views
			</web-resource-name>
			<url-pattern>
				*.jsp
			</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<!-- Nobody is allowed to visit these -->
		</auth-constraint>
	</security-constraint>
</web-app>