<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_1_0.dtd">

<service-builder  package-path="org.opencps.backend.systemlogmgt">
	<namespace>SystemLogMgt</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="SystemLog" remote-service="false" uuid="true" table="opencps_systemlog">

		<!-- PK fields -->
		<column name="logId" type="long" primary="true"></column>
		<!-- Group instance -->
		<column name="groupId" type="long"></column>
        <column name="createDate" type="Date"></column>
        <column name="moduleName" type="String"></column>
        <column name="preLine" type="int"></column>
        <column name="preMethod" type="String"></column>
        <column name="line" type="int"></column>
        <column name="method" type="String"></column>
        <column name="message" type="String"></column>
        <column name="type" type="String"></column>
        <column name="threadId" type="String"></column>

		<!-- Finder methods -->
        
		

		<!-- References -->
        <finder name="multipleGroupId" return-type="Collection">
            <finder-column name="groupId" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="multipleType" return-type="Collection">
            <finder-column name="type" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="multiplemoduleName" return-type="Collection">
            <finder-column name="moduleName" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="multiplePreMethod" return-type="Collection">
            <finder-column name="preMethod" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="multipleMethod" return-type="Collection">
            <finder-column name="method" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="multipleThreadId" return-type="Collection">
            <finder-column name="threadId" comparator="=" arrayable-operator="OR"></finder-column>
        </finder>
        <finder name="likeMessage" return-type="Collection">
            <finder-column name="message" comparator="LIKE"></finder-column>
        </finder>

		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetTag" package-path="com.liferay.portlet.asset" />
	</entity>
</service-builder>