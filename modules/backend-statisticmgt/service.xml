<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="org.opencps.backend.statisticmgt.application">
	<author>khoavu</author>
	<namespace>statistic</namespace>
	<entity local-service="true" name="OpencpsDossierStatisticMgt"
		remote-service="false" table="opencps_statistic_mgt" uuid="true">

		<column name="dossierStatisticMgtId" type="long" primary="true" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="groupId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<column name="month" type="int" />
		<column name="year" type="int" />
		<column name="totalCount" type="int" />
		<column name="processCount" type="int" />
		<column name="remainingCount" type="int" />
		<column name="receivedCount" type="int" />
		<column name="onlineCount" type="int" />
		<column name="onegateCount" type="int" />
		<column name="releaseCount" type="int" />
		<column name="betimesCount" type="int" />
		<column name="ontimeCount" type="int" />
		<column name="overtimeCount" type="int" />
		<column name="doneCount" type="int" />
		<column name="releasingCount" type="int" />
		<column name="processingCount" type="int" />
		<column name="undueCount" type="int" />
		<column name="overdueCount" type="int" />
		<column name="ontimePercentage" type="int" />		
		<column name="domainCode" type="String" />
		<column name="domainName" type="String" />
		<column name="waitingCount" type="int"/>
		<column name="govAgencyCode" type="String" />
		<column name="govAgencyName" type="String" />		
		<column name="domainCode" type="String" />
		<column name="domainName" type="String" />		
		<column name="serviceCode" type="String" />
		<column name="serviceName" type="String" />
		
		<!-- Order -->

		<!-- Filter -->
		<finder return-type="Collection" name="G_UID_Y">
			<finder-column name="groupId"></finder-column>
			<finder-column name="userId"></finder-column>
			<finder-column name="year"></finder-column>
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="M_Y_DM_G">
			<finder-column name="groupId"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="domainCode"/>
			<finder-column name="reporting"/>
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="G_M_Y_G_D">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="G_M_Y_G_D_S">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
			<finder-column name="system"/>
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="M_Y_G">
			<finder-column name="groupId"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
		</finder>

		<finder return-type="OpencpsDossierStatistic" name="GID_M_Y_GAC_S" where="domainCode is null">
			<finder-column name="groupId"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="system"/>			
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="GID_M_Y_S_DC" where="govAgencyCode is null">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="system"/>	
			<finder-column name="domainCode"/>		
		</finder>
		
		<finder return-type="OpencpsDossierStatistic" name="GID_M_Y_GAC" where="domainCode is null and system is null">
			<finder-column name="groupId"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="month"/>
			<finder-column name="year"/>		
		</finder>
		
		
		<finder return-type="OpencpsDossierStatistic" name="GID_M_Y_DC" where="govAgencyCode is null and system is null">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="domainCode"/>		
		</finder>

		<finder return-type="Collection" name="G_M_Y">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
		</finder>

		<finder return-type="Collection" name="CID_GID_Y">
			<finder-column name="companyId"/>
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
		</finder>
		<finder return-type="Collection" name="GID_M_Y_RP">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="reporting"/>
		</finder>
		<finder return-type="Collection" name="GID_MS_Y_RP">
			<finder-column name="groupId"/>
			<finder-column name="month" comparator="=" arrayable-operator="OR"/>
			<finder-column name="year"/>
			<finder-column name="reporting"/>
		</finder>
		<finder return-type="Collection" name="G">
			<finder-column name="groupId"></finder-column>
		</finder>	

		<finder return-type="Collection" name="G_M_Y_GC_DC" where="govAgencyCode is null and domainCode is null">
			<finder-column name="groupId"></finder-column>
			<finder-column name="month"></finder-column>
			<finder-column name="year"></finder-column>
		</finder>	
		
		<finder return-type="Collection" name="G_NM_Y">
			<finder-column name="groupId"></finder-column>
			<finder-column name="month" comparator="!="></finder-column>
			<finder-column name="year"></finder-column>
		</finder>

		<finder return-type="Collection" name="F_REPO">
			<finder-column name="reporting" arrayable-operator="OR"/>
		</finder>
		<finder return-type="Collection" name="F_NOT_GID">
			<finder-column name="groupId" comparator="!="/>
		</finder>
		<finder return-type="Collection" name="M_Y_GOV_DOM_GRO_SYS">
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
			<finder-column name="groupAgencyCode"/>
			<finder-column name="system"/>
		</finder>
		<finder return-type="Collection" name="NOT_G_M_Y_GOV_DOM_GRO_SYS">
			<finder-column name="groupId" comparator="!="/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
			<finder-column name="groupAgencyCode"/>
			<finder-column name="system"/>
		</finder>
		<finder return-type="Collection" name="GID_M_Y_NOT_RP">
			<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="reporting" comparator="!="/>
		</finder>
		<finder return-type="Collection" name="G_Y_GO_DO_GR_SY">
			<finder-column name="groupId"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode" arrayable-operator="OR"/>
			<finder-column name="domainCode"/>
			<finder-column name="groupAgencyCode"/>
			<finder-column name="system"/>
		</finder>
        <finder name="G_M_Y_GOV_DOM_GRO_NOT_SYS" return-type="Collection">
        	<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
			<finder-column name="groupAgencyCode"/>
			<finder-column name="system" comparator="!="/>
        </finder>
        <finder name="G_M_Y_GOV_DOM_GRO_SYS" return-type="Collection">
        	<finder-column name="groupId"/>
			<finder-column name="month"/>
			<finder-column name="year"/>
			<finder-column name="govAgencyCode"/>
			<finder-column name="domainCode"/>
			<finder-column name="groupAgencyCode"/>
			<finder-column name="system"/>
        </finder>
        <finder name="F_M_Y_REPO" return-type="Collection">
        	<finder-column name="month" arrayable-operator="OR"/>
        	<finder-column name="year" arrayable-operator="OR"/>
        	<finder-column name="reporting" arrayable-operator="OR"/>
        </finder>
        <finder name="F_Y_REPO" return-type="Collection">
        	<finder-column name="year" arrayable-operator="OR"/>
        	<finder-column name="reporting" arrayable-operator="OR"/>
        </finder>
	</entity>

</service-builder>