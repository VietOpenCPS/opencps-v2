				<xsd:element name="apiCode" type="xsd:string" />
				<xsd:element name="roleId" type="xsd:long" />
				<xsd:element name="roleCode" type="xsd:string" />
				<xsd:element name="apiRoleStatus" type="xsd:int" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>		
	
	<xsd:element name="ApiRoleResultsModel">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="total" type="xsd:int" minOccurs="0" />
				<xsd:element name="data" type="ApiRoleModel" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
</xsd:schema>

