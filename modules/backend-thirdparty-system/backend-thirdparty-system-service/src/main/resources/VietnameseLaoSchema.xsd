				<xsd:element name="RouteName" type="xsd:string" />
				<xsd:element name="NumberOfLot" type="xsd:string" />
				<xsd:element ref="AttachedFile" minOccurs="1" maxOccurs="unbounded" />
				<xsd:element ref="TransporterSign" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>