				<xsd:element name="DispatchPlace" type="xsd:string" />
				<xsd:element name="OfficialDispatchDate" type="xsd:string" />
				<xsd:element name="Address" type="xsd:string" />
				<xsd:element name="Tel" type="xsd:string" />
				<xsd:element name="Fax" type="xsd:string" />
				<xsd:element name="Email" type="xsd:string" />
				<xsd:element name="Website" type="xsd:string" />
				<xsd:element name="LicenceNo" type="xsd:string" />
				<xsd:element name="LicenceDate" type="xsd:string" />
				<xsd:element name="RouteCode" type="xsd:string" />
				<xsd:element name="RouteName" type="xsd:string" />
				<xsd:element name="FromParkingLot" type="xsd:string" />
				<xsd:element name="FromProvinceCode" type="xsd:string" />
				<xsd:element name="FromProvinceName" type="xsd:string" />
				<xsd:element name="ToParkingLot" type="xsd:string" />
				<xsd:element name="ToProvinceCode" type="xsd:string" />
				<xsd:element name="ToProvinceName" type="xsd:string" />
				<xsd:element name="Distance" type="xsd:string" />
				<xsd:element name="RouteDescription" type="xsd:string" />
				<xsd:element name="ExpImpGateCode" type="xsd:string" />
				<xsd:element name="ExpImpGate" type="xsd:string" />
				<xsd:element ref="VehicleListFixRoad" />
				<xsd:element ref="AttachedFile" minOccurs="1" maxOccurs="unbounded" />
